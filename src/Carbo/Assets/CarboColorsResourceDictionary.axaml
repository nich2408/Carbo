<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <!-- Add Resources Here -->
	<Design.PreviewWith>
		<Border Padding="20">
			<StackPanel Orientation="Vertical" Spacing="20">
				<TextBlock Padding="10" Text="TerraColor" Foreground="White" Background="{StaticResource TerraColor}"></TextBlock>
				<TextBlock Padding="10" Text="TerraColorLight" Foreground="White" Background="{StaticResource TerraColorLight}"></TextBlock>
				<TextBlock Padding="10" Text="TerraColorDark" Foreground="White" Background="{StaticResource TerraColorDark}"></TextBlock>
				<TextBlock Padding="10" Text="ClearColor" Foreground="Black" Background="{StaticResource ClearColor}"></TextBlock>
				<TextBlock Padding="10" Text="SolColor" Foreground="Black" Background="{StaticResource SolColor}"></TextBlock>
				<TextBlock Padding="10" Text="SolColorDark" Foreground="Black" Background="{StaticResource SolColorDark}"></TextBlock>
			</StackPanel>
		</Border>
	</Design.PreviewWith>
	<SolidColorBrush x:Key="TerraColor">#182225</SolidColorBrush>
	<SolidColorBrush x:Key="TerraColorLight">#233136</SolidColorBrush>
	<SolidColorBrush x:Key="TerraColorDark">#121a1c</SolidColorBrush>
	<SolidColorBrush x:Key="ClearColor">#edf2f2</SolidColorBrush>
	<SolidColorBrush x:Key="SolColor">#fdc672</SolidColorBrush>
	<SolidColorBrush x:Key="SolColorDark">#dba95e</SolidColorBrush>
</ResourceDictionary>
